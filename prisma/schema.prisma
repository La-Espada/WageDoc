// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role{
  ADMIN
  COMPANYOWNER
  EMPLOYEE
}

model WageDocUser{
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  email String @unique
  password String 
  birthDate DateTime
  socialInsuranceNumber String
  phoneNumber String
  role Role
  createdAt DateTime?
  updatedAt DateTime?
}
